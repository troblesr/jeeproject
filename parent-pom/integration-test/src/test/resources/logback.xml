<?xml version="1.0" encoding="UTF-8"?>

<configuration>

<!-- SLF4J levels: ERROR, WARN, INFO, DEBUG, TRACE -->

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [-%L] [src/test/java/EmpleadoServiceImpl] [%thread] %-5level %logger{5} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="ERROR">          
    <appender-ref ref="STDOUT" />
  </root>  
  
</configuration>
